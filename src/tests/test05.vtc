varnishtest "Test basic HTTP access vmod (valid cookie, one service, date in the past)"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import access from "${vmod_topbuild}/src/.libs/libvmod_access.so";
	sub vcl_recv {
		set req.http.Cookie = "foo1=bar1; acc=1-service:1345491859-0-...; foo2=bar2";
	}
	sub vcl_deliver {
		set resp.http.result = access.check("service", "acc", "salt");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.result == false
}

client c1 -run

